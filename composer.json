{
  "name": "gigabait93/laravel-extensions",
  "description": "Extensions Laravel system. Documentation: https://github.com/GIGABAIT93/laravel-extensions/tree/main/docs",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "GIGABAIT93",
      "email": "xgigabaitx@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^12.0",
    "illuminate/console": "^12.0",
    "illuminate/database": "^12.0",
    "illuminate/filesystem": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Gigabait93\\Extensions\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gigabait93\\Extensions\\Providers\\ExtensionsServiceProvider"
      ]
    }
  },
  "archive": {
    "exclude": ["docs"]
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "phpstan/phpstan": "^1.11",
    "friendsofphp/php-cs-fixer": "^3.64"
  }
}
